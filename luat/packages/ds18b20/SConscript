Import('RTT_ROOT')
Import('rtconfig')
from building import *

cwd = GetCurrentDir()
src	= Glob('*.c')
CPPPATH = [cwd]

group = DefineGroup('ds18b20', src, depend = [''], CPPPATH = CPPPATH)

Return('group')

